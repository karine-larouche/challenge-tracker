{
  "name": "challenge-tracker",
  "version": "0.1.0",
  "private": true,
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "jest": true
    },
    "extends": [
      "airbnb",
      "prettier/react",
      "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
      "class-methods-use-this": 0,
      "import/no-named-as-default": 0,
      "import/prefer-default-export": 0,
      "no-nested-ternary": 0,
      "react/destructuring-assignment": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/prop-types": [
        "error",
        {
          "ignore": [
            "history",
            "match",
            "location",
            "classes",
            "theme"
          ]
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "all"
        }
      ]
    }
  },
  "dependencies": {
    "@firebase/app-types": "0.4.0",
    "@material-ui/core": "3.3.2",
    "@material-ui/icons": "3.0.1",
    "@rematch/core": "1.0.0-beta.4",
    "@vx/curve": "0.0.165",
    "@vx/glyph": "0.0.170",
    "@vx/grid": "0.0.170",
    "@vx/responsive": "0.0.165",
    "@vx/scale": "0.0.165",
    "@vx/shape": "0.0.170",
    "@vx/tooltip": "0.0.165",
    "d3-array": "1.2.1",
    "date-fns": "2.0.0-alpha.16",
    "firebase": "6.2.4",
    "material-ui-pickers": "1.0.1",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-calendar-heatmap": "1.6.3",
    "react-dom": "16.8.6",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "3.0.1",
    "react-swipeable-views": "0.13.0",
    "redux": "4.0.0",
    "typeface-roboto": "0.0.54",
    "typescript": "3.5.2"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.14.2",
    "firebase-tools": "7.0.2",
    "prettier": "1.13.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "./node_modules/.bin/eslint --fix ./src",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
